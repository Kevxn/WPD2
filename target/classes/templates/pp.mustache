</nav>

<br> <br>

<div class="jumbotron jumbotron-fluid">
    <div class="container">
        <h1 class="display-4">Planners</h1>
    </div>
</div>


<div class="container">
    <span class="oi oi-home"></span>


    <h1>Choose a planner to work on</h1>
    <ol>
    {{#plannerList}}
       <li> {{plannerName}}</li>
    {{/plannerList}}
    </ol>
    <form method="POST" action="/add3">
        <label for="id">Enter ID</label>
        <input type="text" class="form-control" id="id" name="id" placeholder="enter id">
        <br>
        <button type="submit" class="btn btn-primary" value="add3">Create</button>
    </form>





</div>

